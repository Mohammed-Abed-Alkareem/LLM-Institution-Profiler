{
  "test_suite_name": "Institution Profiler Comprehensive Benchmarks",
  "description": "Complete benchmarking test suite for search, crawling, and data processing",
  "version": "1.0",
  "test_configurations": [
    {
      "test_name": "search_performance_test",
      "test_description": "Test search performance across different institution types",
      "category": "search",
      "institutions": [
        {
          "institution_name": "Harvard University",
          "institution_type": "university",
          "expected_min_quality": 0.8,
          "expected_max_cost": 0.01,
          "expected_max_time": 3.0
        },
        {
          "institution_name": "Mayo Clinic",
          "institution_type": "hospital",
          "expected_min_quality": 0.8,
          "expected_max_cost": 0.01,
          "expected_max_time": 3.0
        },
        {
          "institution_name": "JPMorgan Chase",
          "institution_type": "bank",
          "expected_min_quality": 0.8,
          "expected_max_cost": 0.01,
          "expected_max_time": 3.0
        }
      ],
      "iterations": 3,
      "parallel_execution": false,
      "test_parameters": {
        "force_api": false,
        "max_results": 10,
        "enable_caching": true
      }
    },
    {
      "test_name": "crawling_efficiency_test",
      "test_description": "Test web crawling efficiency and content extraction quality",
      "category": "crawling",
      "institutions": [
        {
          "institution_name": "MIT",
          "institution_type": "university",
          "test_urls": [
            "https://web.mit.edu/",
            "https://web.mit.edu/about/",
            "https://admissions.mit.edu/"
          ],
          "expected_min_quality": 0.7,
          "expected_max_cost": 0.0,
          "expected_max_time": 30.0
        }
      ],
      "iterations": 2,
      "parallel_execution": true,
      "test_parameters": {
        "max_pages": 3,
        "content_extraction": "comprehensive",
        "enable_caching": true
      }
    },
    {
      "test_name": "data_format_comparison",
      "test_description": "Compare different data output formats for efficiency and quality",
      "category": "pipeline",
      "institutions": [
        {
          "institution_name": "Stanford University",
          "institution_type": "university",
          "expected_min_quality": 0.8,
          "expected_max_cost": 0.05,
          "expected_max_time": 10.0
        }
      ],
      "data_formats": ["json", "xml", "structured"],
      "iterations": 1,
      "parallel_execution": false,
      "test_parameters": {
        "skip_extraction": true,
        "format_testing": true
      }
    },
    {
      "test_name": "cost_optimization_test",
      "test_description": "Test cost optimization strategies across the pipeline",
      "category": "cost",
      "institutions": [
        {
          "institution_name": "University of California Berkeley",
          "institution_type": "university",
          "expected_min_quality": 0.7,
          "expected_max_cost": 0.02,
          "expected_max_time": 15.0
        }
      ],
      "cost_strategies": [
        {
          "name": "cache_first",
          "description": "Prioritize cache usage",
          "parameters": {
            "force_cache": true,
            "cache_similarity_threshold": 0.8
          }
        },
        {
          "name": "api_minimal",
          "description": "Minimize API calls",
          "parameters": {
            "max_api_calls": 1,
            "prefer_cached_results": true
          }
        }
      ],
      "iterations": 2,
      "parallel_execution": false
    },
    {
      "test_name": "latency_performance_test",
      "test_description": "Test latency performance under different load conditions",
      "category": "latency",
      "institutions": [
        {
          "institution_name": "Oxford University",
          "institution_type": "university",
          "expected_min_quality": 0.8,
          "expected_max_cost": 0.01,
          "expected_max_time": 5.0
        },
        {
          "institution_name": "Cambridge University",
          "institution_type": "university",
          "expected_min_quality": 0.8,
          "expected_max_cost": 0.01,
          "expected_max_time": 5.0
        }
      ],
      "load_conditions": [
        {
          "name": "sequential",
          "description": "Sequential processing",
          "concurrent_requests": 1
        },
        {
          "name": "parallel_light",
          "description": "Light parallel processing",
          "concurrent_requests": 2
        }
      ],
      "iterations": 3,
      "parallel_execution": true
    },
    {
      "test_name": "quality_validation_test",
      "test_description": "Validate data quality and completeness across different institutions",
      "category": "quality",
      "institutions": [
        {
          "institution_name": "Johns Hopkins University",
          "institution_type": "university",
          "expected_min_quality": 0.85,
          "expected_max_cost": 0.03,
          "expected_max_time": 20.0
        },
        {
          "institution_name": "Cleveland Clinic",
          "institution_type": "hospital",
          "expected_min_quality": 0.85,
          "expected_max_cost": 0.03,
          "expected_max_time": 20.0
        }
      ],
      "quality_metrics": [
        "information_completeness",
        "data_accuracy",
        "field_coverage",
        "validation_success_rate"
      ],
      "iterations": 2,
      "parallel_execution": false,
      "test_parameters": {
        "enable_extraction": true,
        "enable_validation": true,
        "quality_threshold": 0.8
      }
    }
  ],
  "global_settings": {
    "timeout_seconds": 300,
    "retry_attempts": 2,
    "log_level": "INFO",
    "save_detailed_results": true,
    "generate_comparison_report": true,
    "output_directory": "project_cache/test_results"
  },
  "reporting": {
    "generate_html_report": true,
    "generate_json_summary": true,
    "generate_csv_export": true,
    "include_performance_graphs": true,
    "include_cost_breakdown": true
  }
}
