{
  "test_suite_name": "Quick Performance Test",
  "description": "Quick test for basic functionality and performance",
  "version": "1.0",
  "test_configurations": [
    {
      "test_name": "quick_search_test",
      "test_description": "Quick search performance test",
      "category": "search",
      "institutions": [
        {
          "institution_name": "MIT",
          "institution_type": "university",
          "expected_min_quality": 0.7,
          "expected_max_cost": 0.01,
          "expected_max_time": 5.0
        }
      ],
      "iterations": 1,
      "parallel_execution": false,
      "test_parameters": {
        "force_api": false,
        "enable_caching": true
      }
    },
    {
      "test_name": "basic_crawl_test",
      "test_description": "Basic crawling test",
      "category": "crawling",
      "institutions": [
        {
          "institution_name": "Harvard University",
          "institution_type": "university",
          "test_urls": ["https://www.harvard.edu/"],
          "expected_min_quality": 0.6,
          "expected_max_cost": 0.0,
          "expected_max_time": 15.0
        }
      ],
      "iterations": 1,
      "parallel_execution": false,
      "test_parameters": {
        "max_pages": 1,
        "enable_caching": true
      }
    }
  ],
  "global_settings": {
    "timeout_seconds": 60,
    "retry_attempts": 1,
    "log_level": "INFO",
    "save_detailed_results": true,
    "output_directory": "project_cache/test_results"
  }
}
